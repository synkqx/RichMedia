package com.example.richmedia;

public class POI 
{
	float timestamp;
	float longitude;
	float latitude;
	actor actors[];
	int cptActors = 0;
	
	
	public POI(float timestamp, float longitude, float latitude, int nbActors) {
		super();
		this.timestamp = timestamp;
		this.longitude = longitude;
		this.latitude = latitude;
		actors = new actor[nbActors];
	}
	
	public fullFillActors(Actor acteur)
	{
		actors[cptActors] = acteur;
		cptActors++;
	}
	
	public float getTimestamp() {
		return timestamp;
	}
	public void setTimestamp(float timestamp) {
		this.timestamp = timestamp;
	}
	public float getLongitude() {
		return longitude;
	}
	public void setLongitude(float longitude) {
		this.longitude = longitude;
	}
	public float getLatitude() {
		return latitude;
	}
	public void setLatitude(float latitude) {
		this.latitude = latitude;
	}
}
